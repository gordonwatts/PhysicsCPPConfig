<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<!--Configure the actual build.-->
	<ItemDefinitionGroup>
		<ClCompile>
			<DisableSpecificWarnings>4244</DisableSpecificWarnings>
